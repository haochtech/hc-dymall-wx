<block tt:if='{{showModal}}'><view bindtap='hideModal' catchtouchmove='preventTouchMove' id='modal-mask'></view></block><block tt:if='{{showModal}}'><view class='modal-dialog'><view bindtap='close_box'><image class='close3' src='{{__wxapp_img.store.close3.url}}'></image></view><view class='modal-title' style='overflow: hidden; white-space: nowrap; text-overflow: ellipsis;'>{{goods_name}}</view><scroll-view scrollY='true' style='height: 100%;'><view class='modal-content'><view><block tt:for='{{attr_group_list}}' tt:for-item='attr_group' tt:for-index='index' tt:key='{{attr_group.id}}'><block tt:if='{{attr_group.attr_list.length > 0}}'><view class='attr-group'><view class='attr-group-name'>{{attr_group.attr_group_name}} :</view><view class='attr-list'><block tt:for='{{attr_group.attr_list}}' tt:for-item='item' tt:for-index='index' tt:key='{{item.id}}'><text bindtap='attrClick' class='attr_name {{(item.checked?"select_active":"")}} {{(item.is_attr_num?"attr_num_0":"")}}' data-group-id='{{attr_group.attr_group_id}}' data-id='{{item.attr_id}}'>{{item.attr_name}}</text></block></view></view></block></block></view></view></scroll-view><view class='modal-footer'><block tt:if='{{check_goods_price}}'><view class='guigeprice'>￥{{check_goods_price}}</view></block><block tt:else><view class='guigeprice'>￥{{temporaryGood.price}}</view></block><block tt:if='{{check_num}}'><view class='btn-car_num'><image bindtap='guigejian' class='carStandard' src='{{__wxapp_img.store.jian.url}}'></image><input bindinput='goodNumChange' class='carStandard good_num' id='num' maxlength='999' type='number' value='{{check_num}}'></input><image bindtap='onConfirm' class='carStandard' src='{{__wxapp_img.store.jia.url}}'></image></view></block><block tt:else><view bindtap='onConfirm' class='btn-car' data-status='confirm' style='background:#FF4544;color:#FFFFFF;'>加入购物车</view></block></view></view></block>