<block tt:if='{{ (copyright &&  (copyright.icon || copyright.text) ) }}'><block tt:if='{{copyright.is_phone == 1}}'><view bindtap='navigatorClick' data-open_type='tel' data-tel='{{(copyright.phone?copyright.phone:contact_tel)}}' style='padding: 24rpx'><view style='text-align: center;font-size: 0;'><block tt:if='{{copyright.icon}}'><image mode='aspectFit' src='{{copyright.icon}}' style='height: 60rpx;width: 240rpx'></image></block></view><view style='text-align: center;line-height: 1'><block tt:if='{{copyright.text}}'><text style='color: rgba(0,0,0,.3);font-size: 9pt;'>{{copyright.text}}</text></block></view></view></block><block tt:elif='{{copyright.open_type == "wxapp"}}'><view style='padding: 24rpx'><navigator appId='{{copyright.appId}}' hoverClass='none' openType='navigate' path='{{copyright.path}}' target='miniProgram'><view style='text-align: center;font-size: 0;'><block tt:if='{{copyright.icon}}'><image data-url='{{copyright.url}}' mode='aspectFit' src='{{copyright.icon}}' style='height: 60rpx;width: 240rpx'></image></block></view><view style='text-align: center;line-height: 1'><block tt:if='{{copyright.text}}'><text bindtap='openWxapp' data-url='{{copyright.url}}' style='color: rgba(0,0,0,.3);font-size: 9pt;'>{{copyright.text}}</text></block></view></navigator></view></block><block tt:elif='{{copyright.open_type == "navigate"}}'><view style='padding: 24rpx'><view style='text-align: center;font-size: 0;'><navigator style='display: inline-block' url='{{copyright.url}}'><block tt:if='{{copyright.icon}}'><image mode='aspectFit' src='{{copyright.icon}}' style='height: 60rpx;width: 240rpx'></image></block></navigator></view><view style='text-align: center;line-height: 1'><block tt:if='{{copyright.text}}'><navigator style='color: rgba(0,0,0,.3);font-size: 9pt;display: inline-block' url='{{copyright.url}}'><text>{{copyright.text}}</text></navigator></block></view></view></block><block tt:else><view style='padding: 24rpx'><view style='text-align: center;font-size: 0;'><block tt:if='{{copyright.icon}}'><image mode='aspectFit' src='{{copyright.icon}}' style='height: 60rpx;width: 240rpx'></image></block></view><view style='text-align: center;line-height: 1'><block tt:if='{{copyright.text}}'><text style='color: rgba(0,0,0,.3);font-size: 9pt;'>{{copyright.text}}</text></block></view></view></block></block><block tt:else><block tt:if='{{ (store.copyright || store.copyright_pic_url) }}'><view class='flex-y-center flex-x-center flex-col' style='padding: 20rpx'><block tt:if='{{store.copyright_pic_url}}'><view><navigator url='{{store.copyright_url}}'><image mode='aspectFit' src='{{store.copyright_pic_url}}' style='height: 60rpx;width: 240rpx'></image></navigator></view></block><block tt:if='{{store.copyright}}'><view style='color: rgba(0,0,0,.3);font-size: 9pt;'><navigator url='{{store.copyright_url}}'><text>{{copyright.text}}</text></navigator></view></block></view></block></block>