<view class='page' style='position: relative;'><include src='/components/common/common'></include><include src='/components/header/header'></include><view class='body after-navber'><view style='display: flex;flex-direction: column;height:100%;width: 100%;'><view style='flex-grow: 0;'><form><view class='search-box'><view class='input-box'><view bindtap='pullDown' class='flex-row pull-down'><view class='cat'>{{default_cat.name}}</view><view style='font-size: 0'><image class='search-icon' src='{{__wxapp_img.search.s_up.url}}'></image></view><block wx:if='{{is_show}}'><view class='pull-down-list'><block wx:for='{{cats}}' wx:for-item='item' wx:for-index='index' wx:key='{{item.id}}'><view bindtap='change_cat' class='pull-down-text flex-x-center {{(item.is_active?"active":"")}}' data-id='{{item.id}}'>{{item.name}}</view></block></view></block></view><view style='width: 75%'><input autoFocus='true' bindblur='inputBlur' bindconfirm='inputConfirm' bindfocus='inputFocus' class='search-input' confirmType='search' style='width: 100%'></input></view></view><view><view bindtap='searchCancel' class='search-cancel'>取消</view></view></view></form></view><view style='flex-grow: 3;position: relative'><block wx:if='{{ ( (show_history && history_list)  && history_list.length > 0) }}'><view class='search-history'><view><text class='search-history-title'>搜索历史</text><view bindtap='deleteSearchHistory' class='delete-search-history'><image src='{{__wxapp_img.store.delete.url}}' style='width: 28rpx;height: 34rpx'></image></view></view><view class='search-history-list'><block wx:for='{{history_list}}' wx:for-item='item' wx:for-index='index' wx:key='{{item.id}}'><view bindtap='historyClick' class='search-history-item' data-value='{{item.keyword}}'>{{item.keyword}}                                </view></block></view></view></block><block wx:if='{{show_result}}'><block wx:if='{{is_search}}'><view class='search-result'><view class='goods-list' style='padding-top: 20rpx'><block wx:for='{{goods_list}}' wx:for-item='item' wx:for-index='index' wx:key='{{item.id}}'><navigator class='goods-item' openType='navigate' url='{{item.url}}'><image class='goods-pic' mode='aspectFill' src='{{item.pic_url}}'></image><view class='goods-info'><text class='goods-name'>{{item.name}}</text><text class='goods-price'>￥{{item.price}}</text></view></navigator></block></view><view class='loading-more-bar {{loading_more_active}}'><image src='{{__wxapp_img.system.loading_black.url}}' style='width: 48rpx;height: 48rpx'></image></view></view></block><block wx:else><view class='search-result'><view class='flex-row flex-x-center' style='padding:36rpx 0;'><view class='flex-row flex-y-center'><view class='flex-grow-0' style='width:100rpx;height:100rpx;margin-right:40rpx;'><image src='{{__wxapp_img.search.search_no.url}}' style='width:100rpx;height:100rpx;'></image></view><view class='flex-grow-1' style='color:#353535;'>抱歉，没有相关商品</view></view></view></view></block></block></view></view></view><include src='/components/footer/footer'></include></view>