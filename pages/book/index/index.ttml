<view class='page {{__page_classes}}'><include src='/components/common/common'></include><include src='/components/header/header'></include><view class='body after-navber'><block wx:if='{{ (cat_show == 1 && cid_url) }}'><scroll-view class='scroll-view-x' scrollIntoView='{{toView}}' scrollLeft='{{scrollLeft}}' scrollWithAnimation='true' scrollX='true' style='width:100%;'><view bindtap='switchNav' class='scroll-view-item {{(cid == 0?"active":"")}}' data-id='0' id='0'>                全部            </view><block wx:for='{{cat}}' wx:for-item='item' wx:for-index='index' wx:key='unique'><view bindtap='switchNav' class='scroll-view-item {{(cid == item.id?"active":"")}}' data-id='{{item.id}}' id='{{item.id}}'>                    {{item.name}}                </view></block></scroll-view></block><scroll-view scrollY='{{true}}' class='after-navber' scrollIntoView='{{toView}}' scrollTop='{{scrollTop}}' style='{{( (cat_show == 1 && cid_url) ?"padding-top: 100rpx;":"")}};height: 100%;'><view class='goods'><block wx:for='{{goods}}' wx:for-item='item' wx:for-index='index' wx:key='{{item.id}}'><navigator class='goods-item' url='/pages/book/details/details?id={{item.id}}'><view class='goods-pic'><image src='{{item.cover_pic}}'></image></view><view class='goods-title'>                        {{item.name}}                    </view><view class='price flex-row flex-y-center'><block wx:if='{{item.price != 0}}'><view class='money flex-grow-1 flex-y-center'>                            ¥ <view style='max-width:180rpx'>{{item.price}}</view></view></block><block wx:if='{{item.price == 0}}'><view class='money flex-grow-1 flex-y-center'>                            免费预约                        </view></block><view class='go-book flex-gorw-0 flex-x-center flex-y-center'>                            预约                        </view></view></navigator></block></view><view class='loading-bar {{(show_loading_bar?"active":"")}}' style='text-align: center;{{(emptyGoods == 1?"display:none":"")}}'><image src='{{__wxapp_img.system.loading.url}}' style='width: 160rpx;height: 80rpx'></image></view><view class='empty-bar flex-row flex-x-center flex-y-center' style='{{(emptyGoods != 1?"display:none":"")}}'>                没有更多商品了~            </view></scroll-view></view><include src='/components/quick-navigation/quick-navigation.ttml'></include><include src='/components/footer/footer'></include></view>